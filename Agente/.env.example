# Example environment file for the IoT Agent
# Copy to .env and fill in real values as needed

# Encryption token for AES-GCM (required)
AGENT_TOKEN=CHANGE_ME

# Orion/Context Broker base URL (inside docker-compose network use service name)
ORION_BASE=http://orion:1026

# Entity IDs (fixed defaults)
GPS_ENTITY_ID=gps_oruga
TEMP_ENTITY_ID=temp_oruga
HUM_ENTITY_ID=hum_oruga

# Flask settings
FLASK_ENV=development
